---
import getTitle from "@server/actions/GetTitle"
import type {RelatedPages} from "@server/Utils"
export interface Props { info?: RelatedPages }
const { info } = Astro.props

const parts = ["Degaussing Comic", ...getTitle({path: Astro.url.pathname, info})]
//TODO parts excluded by part text is fishy
---
<title>{parts.filter(part => part !== "Home" && part !== "Inicio").join(" | ")}</title>
