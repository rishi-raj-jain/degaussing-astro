---
import StripAsideLayout from '@web/layouts/StripAsideLayout.astro'
import getAllPages from "@server/actions/GetAllPages"

export async function getStaticPaths() {
    return await getAllPages()
}

const { image, chapter, page } = Astro.props;
---

<StripAsideLayout title="Degaussing Comic" chapter={chapter} page={page}>
    <img class="comic-strip" src={image} alt="wea">
</StripAsideLayout>
<style>
    .comic-strip {
        width: 862px;
        height: 1230px;
    }
</style>

