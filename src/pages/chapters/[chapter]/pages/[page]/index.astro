---
import StripAsideLayout from '@web/layouts/StripAsideLayout.astro'
import getAllPages from "@server/actions/GetAllPages"

export async function getStaticPaths() {
    return await getAllPages()
}

const info = Astro.props;
---

<StripAsideLayout title="Degaussing Comic" info={info}>
    <img id="strip" class="comic-strip" src={info.image} alt="wea"/>
</StripAsideLayout>
<style>
    .comic-strip {
        width: 862px;
        height: 1230px;
    }
</style>

