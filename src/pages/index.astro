---
import i18next, { changeLanguage } from "i18next";
import { Image } from "@astrojs/image/components";
import StripAsideLayout from "@web/layouts/StripAsideLayout.astro";
import getLastPage from "@server/actions/GetLastPage";

changeLanguage("en-US");

const info = await getLastPage({ language: i18next.language });
---

<StripAsideLayout title="Degaussing Comic" info={info}>
   <Image src={info.image} alt="wea" width="862" height="1230" format="avif"/>
</StripAsideLayout>
