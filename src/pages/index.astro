---
import StripAsideLayout from '@web/layouts/StripAsideLayout.astro'
import getLastPage from '@server/actions/GetLastPage'

const info = await getLastPage()
---

<StripAsideLayout title="Degaussing Comic" info={info}>
    <img class="comic-strip" src={info.image} alt="wea">
</StripAsideLayout>
<style>
    .comic-strip {
        width: 862px;
        height: 1230px;
    }
</style>
