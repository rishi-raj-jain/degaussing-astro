---
import StripAsideLayout from '@web/layouts/StripAsideLayout.astro'
import getLastPage from '@server/actions/GetLastPage'

const {image} = await getLastPage()
---

<StripAsideLayout title="Degaussing Comic">
    <img class="comic-strip" src={image} alt="wea">
</StripAsideLayout>
<style>
    .comic-strip {
        width: 862px;
        height: 1230px;
    }
</style>
